        </record>

        <!-- Default Kenya Statutory Config -->
        <record id="kenya_statutory_config_default" model="kenya.statutory.config">
            <field name="name">Kenya Statutory Rates 2024</field>
            <field name="nssf_tier1_limit">7000</field>
            <field name="nssf_tier2_limit">36000</field>
            <field name="nssf_rate">6</field>
            <field name="housing_levy_rate">1.5</field>
            <field name="personal_relief">2400</field>
            <field name="insurance_relief_rate">15</field>
            <field name="max_insurance_relief">5000</field>
        </record>
    </data>
</odoo>
