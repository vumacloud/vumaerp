<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- PAYE Tax Bands 2024 (Monthly) -->
        <record id="paye_band_1" model="kenya.paye.tax.band">
            <field name="min_income">0</field>
            <field name="max_income">24000</field>
            <field name="rate">10</field>
        </record>
        <record id="paye_band_2" model="kenya.paye.tax.band">
            <field name="min_income">24000</field>
            <field name="max_income">32333</field>
            <field name="rate">25</field>
        </record>
        <record id="paye_band_3" model="kenya.paye.tax.band">
            <field name="min_income">32333</field>
            <field name="max_income">500000</field>
            <field name="rate">30</field>
        </record>
        <record id="paye_band_4" model="kenya.paye.tax.band">
            <field name="min_income">500000</field>
            <field name="max_income">800000</field>
            <field name="rate">32.5</field>
