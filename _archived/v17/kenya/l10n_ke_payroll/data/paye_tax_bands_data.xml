<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Kenya PAYE Tax Bands 2024 (Monthly) -->
        <record id="paye_band_1" model="kenya.paye.tax.band">
            <field name="name">Band 1 - 10%</field>
            <field name="lower_limit">0</field>
            <field name="upper_limit">24000</field>
            <field name="rate">10</field>
        </record>

        <record id="paye_band_2" model="kenya.paye.tax.band">
            <field name="name">Band 2 - 25%</field>
            <field name="lower_limit">24000</field>
            <field name="upper_limit">32333</field>
            <field name="rate">25</field>
        </record>

        <record id="paye_band_3" model="kenya.paye.tax.band">
            <field name="name">Band 3 - 30%</field>
            <field name="lower_limit">32333</field>
            <field name="upper_limit">500000</field>
            <field name="rate">30</field>
        </record>

        <record id="paye_band_4" model="kenya.paye.tax.band">
            <field name="name">Band 4 - 32.5%</field>
            <field name="lower_limit">500000</field>
            <field name="upper_limit">800000</field>
            <field name="rate">32.5</field>
        </record>

        <record id="paye_band_5" model="kenya.paye.tax.band">
            <field name="name">Band 5 - 35%</field>
            <field name="lower_limit">800000</field>
            <field name="upper_limit">999999999</field>
            <field name="rate">35</field>
        </record>
    </data>
</odoo>
